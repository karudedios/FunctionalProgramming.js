{
  "name": "functional-programming.js",
  "version": "1.0.0",
  "description": "Functional Programming in Javascript",
  "main": "src/FunctionalProgramming.js",
  "scripts": {
    "test": "jasmine-node spec/",
    "test:watch": "jasmine-node spec/ --color --autotest --watch src/",
    "minify": "uglifyjs src/FunctionalProgramming.js --output dist/FunctionalProgramming.min.js --comments --verbose",
    "jshint": "npm run jshint:src && npm run jshint:test",
    "jshint:src": "jshint src/",
    "jshint:test": "jshint test/",
    "build": "npm run test && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/karudedios/FunctionalProgramming.js.git"
  },
  "author": "cdedios",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karudedios/FunctionalProgramming.js/issues"
  },
  "homepage": "https://github.com/karudedios/FunctionalProgramming.js#readme",
  "devDependencies": {
    "jasmine-node": "^1.14.5",
    "jshint": "^2.8.0",
    "uglify-js": "^2.4.24"
  }
}
